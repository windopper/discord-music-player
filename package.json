{
  "name": "discord-music-player",
  "version": "9.1.1",
  "description": "Complete framework to facilitate music commands using discord.js v14 and v13.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "test": "ts-jest test/test.ts",
    "docs:test": "docgen --jsdoc jsdoc.json --source src/*.ts src/**/*.ts --custom docs/custom.yml",
    "docs": "docgen --jsdoc jsdoc.json --source src/*.ts src/**/*.ts --custom docs/custom.yml --output docs/docs.json"
  },
  "keywords": [
    "music",
    "player",
    "bot",
    "framework",
    "discord",
    "youtube",
    "spotify",
    "v14",
    "v13"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windopper/discord-music-player.git"
  },
  "author": "SushiBtw",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.16.1",
    "domhandler": "^5.0.3",
    "ffmpeg-static": "^5.2.0",
    "htmlparser2": "^9.0.0",
    "isomorphic-unfetch": "^4.0.2",
    "libsodium-wrappers": "^0.7.13",
    "play-dl": "^1.9.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "assert-diff": "^3.0.3",
    "discord.js": "^14.14.1",
    "discord.js-docgen": "github:discordjs/docgen#ts-patch",
    "eslint": "^8.56.0",
    "jsdoc-babel": "^0.5.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  }
}
